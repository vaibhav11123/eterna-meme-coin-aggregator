        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "https://eterna-aggregator.onrender.com/api/health",
              "protocol": "https",
              "host": ["eterna-aggregator", "onrender", "com"],
              "path": ["api", "health"]
            },
            "description": "Basic health check endpoint"
          }
        },
        {
          "name": "Status (Comprehensive)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "https://eterna-aggregator.onrender.com/api/status",
              "protocol": "https",
              "host": ["eterna-aggregator", "onrender", "com"],
              "path": ["api", "status"]
